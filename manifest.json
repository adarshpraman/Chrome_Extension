{
    "manifest_version": 3,
    "version": "1.1",
    "name": "Leads Tracker",
    "description": "An extension that helps you keep a track of your leads.",
    "action": {
        "default_title": "Leads Tracker",
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "tabs"
    ]
}